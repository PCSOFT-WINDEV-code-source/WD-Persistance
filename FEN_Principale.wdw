     code : |1-
      // Résumé : Ouvre la fenêtre d'optimisation
      // Syntaxe :
      // _Ouvre_Optimisation ()
      //
      // Paramètres :
      //	Aucun
      // Valeur de retour :
      // 	Aucune
      //
      Procédure _Ouvre_Optimisation()
      
      // Variables locales
      i est un entier
      
      i = 1
      // Tant que la fenêtre FenOptim renvoie vrai, cela veut dire que l'on doit la réouvrir
      TANTQUE Ouvre(FEN_Optim,i)
      	i++
      FIN
      
      // Affichage d'un message
      Info(<§$0007§>,<§$0008§>)
