  procedures :
   -
     name : MAJModeSauvegarde
     procedure_id : 1486456003291831672
     type_code : 14
     code : |1-
      // Syntaxe :
      //MAJModeSauvegarde ()
      //
      // Cette procédure permet de mettre à jour le libellé contenant le mode de sauvegarde actuel
      
      Procédure MAJModeSauvegarde()
      
      sModeSauvegarde est une chaine = <§$0002§>
      
      // Selon le type de sauvegarde défini actuellement
      // On le compare aux constantes du W-Langage utilisées avec la fonction InitParamètre
      selon pGlobales.gnTypeSauvegarde
      	
      	cas paramRegistre // Base de registre
      		sModeSauvegarde = ChaîneConstruit(sModeSauvegarde,<§$0003§>)
      		
      	cas paramxml // Fichier XML
      		sModeSauvegarde = ChaîneConstruit(sModeSauvegarde,<§$0004§>)
      		
      	cas paramini // Fichier INI
      		sModeSauvegarde = ChaîneConstruit(sModeSauvegarde,<§$0005§>)
      		
      	autres cas 
      		// Non géré
      		
      FIN
      // Vérification du chemin de sauvegarde
      si pGlobales.gsCheminSauvegarde = "" ALORS
      	sModeSauvegarde = chaineconstruit(sModeSauvegarde,<§$0006§>)
      sinon
      	sModeSauvegarde = chaineconstruit(sModeSauvegarde, pGlobales.gsCheminSauvegarde)	
      FIN
      
      
      LIB_MODE_SAUVEGARDE = sModeSauvegarde
     type : 458752
