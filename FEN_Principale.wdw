   -
     code : |1-
      // --------------------------------------------
      // GESTION DES PARAMETRES DE L'EXEMPLE
      // --------------------------------------------
      
      // Récupération des paramètres de configuration de l'exemple
      InitParamètre(ParamIni, CS_CONFIG_EXEMPLE)
      
      // Chargement des paramètres de sauvegarde
      pGlobales.gnTypeSauvegarde = ChargeParamètre(CS_TYPE_SAUVEGARDE, -1)
      pGlobales.gscheminSauvegarde = ChargeParamètre(CS_CHEMIN_SAUVEGARDE)
      
      si EnModeTestAutomatique() ALORS
      	pGlobales.gnTypeSauvegarde = paramIni
      	pGlobales.gsCheminSauvegarde = CS_FICHIER_INI
      FIN
      
      // Si aucun type de sauvegarde
      si pGlobales.gnTypeSauvegarde = -1 ALORS
      	
      	info(<§$0000§>)
      	
      	// C'est le premier lancement de l'exemple, on doit renseigner les paramètres de configuration de l'exemple
      	ouvre(FEN_Config, Vrai)	
      	
      	// Paramètres enregistrés
      	si pGlobales.gnTypeSauvegarde = -1 ALORS
      		erreur(<§$0001§>)
      		finprogramme()
      	FIN
      	
      FIN
      
      
      // ---------------------------------------------
      // Initialisation des paramètres de sauvegarde
      // ---------------------------------------------
      
      // Initialisation des derniers paramètres
      InitParamètre(pGlobales.gnTypeSauvegarde, pGlobales.gsCheminSauvegarde)	
