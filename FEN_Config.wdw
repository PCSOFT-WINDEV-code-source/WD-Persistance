      sNomPersonnel est une chaine // Le nom personnel du fichier
      
      sMessage = <¬ß$0000¬ß>
      
      // -----------------------------------------------------
      // Construction de la premi√®re partie de la description
      // -----------------------------------------------------
      
      // Selon le type de sauvegarde
      selon SEL_TYPE_SAVE
      	
      	cas 1 // Base de registre
      		
      		// On est dans une cl√© de registre
      		bCl√©Registre = vrai
      		
      		// Construction de la premi√®re partie de la description
      		sMessage = Cha√ÆneConstruit(sMessage, <¬ß$0001¬ß>, <¬ß$0002¬ß>, <¬ß$0003¬ß>, <¬ß$0004¬ß>)
      		
      		
