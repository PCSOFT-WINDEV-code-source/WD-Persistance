  procedures :
   -
     name : ChangeDescription
     procedure_id : 1486460470018096152
     type_code : 14
     code : |1+
      // Syntaxe :
      //ChangeDescription ()
      //
      // Cette procédure permet de mettre à jour la description en fonction du paramétrage de la sauvegarde
      // 
      Procédure ChangeDescription()
      
      sMessage est une chaine // Le message que l'on va afficher dans le champ de la description
      bCléRegistre est un booléen // Est ce que la sauvegarde se fait dans une clé de registre ?
      sParamètre4 est une chaine // Le paramètre 4 correspond à l'endroit où sont sauvegardés les paramètres (Clé de registre ou nom du fichier)
      sNomPersonnel est une chaine // Le nom personnel du fichier
      
      sMessage = <§$0000§>
      
      // -----------------------------------------------------
      // Construction de la première partie de la description
      // -----------------------------------------------------
      
      // Selon le type de sauvegarde
      selon SEL_TYPE_SAVE
      	
      	cas 1 // Base de registre
      		
      		// On est dans une clé de registre
      		bCléRegistre = vrai
      		
      		// Construction de la première partie de la description
      		sMessage = ChaîneConstruit(sMessage, <§$0001§>, <§$0002§>, <§$0003§>, <§$0004§>)
      		
      		
      	cas 2 // Fichier XML
      		
      		// Construction de la première partie de la description
      		sMessage = ChaîneConstruit(sMessage, <§$0005§>, <§$0006§>, <§$0007§>, <§$0004§>)
      		
      		// Sauvegarde du nom de fichier en cas de gestion manuelle
      		sNomPersonnel = CS_FICHIER_XML
      		
      	cas 3 // Fichier INI
      		
      		// Construction de la première partie de la description
      		sMessage = ChaîneConstruit(sMessage, <§$0008§>, <§$0009§>, <§$0007§>, <§$0004§>)
      		
      		// Sauvegarde du nom de fichier en cas de gestion manuelle
      		sNomPersonnel = CS_FICHIER_ini
      		
      	autres cas 
      		// Le cas n'est pas géré
      		sMessage = ""
      		
      FIN
      
      // -----------------------------------------------------
      // Construction de la seconde partie de la description
      // -----------------------------------------------------
      
      // Selon le mode de gestion de sauvegarde des paramètres
      Selon SEL_GESTION_SAVE
      	cas 1 // Gestion des paramètres Automatique
      		
      		// Est-on dans la base de registre
      		si bCléRegistre = Vrai ALORS
      			// Récupération de la clé correspondant au projet
      			sParamètre4 = projetinfo(piregistre)
      		
      		sinon
      			// Le nom du fichier de sauvegarde est le nom du projet
      			sParamètre4 = frepexe() + <§$000a§> + fextraitchemin(sNomPersonnel, fExtension)
      			
      		FIN
      		
      		// Construction de la seconde partie de la description
      		sMessage = Chaineconstruit(sMessage, <§$000b§>, sParamètre4)
      		sMessage += RC+ RC + <§$000c§>
      			
      				
      	cas 2 // Gestion des paramètres manuelles (spécifications des chemins de sauvegarde manuelle)
      			
      		si bCléRegistre = Vrai ALORS
      			// on est dans la base de registre donc sauvegarde des paramètres dans la clé personnelle
      			sParamètre4 = CS_REGISTRE
      		sinon
      			// On est dans un fichier
      			sParamètre4 = sNomPersonnel		
      			
      		FIN
      		
      		
      		// Construction de la seconde partie de la description
      		sMessage = Chaineconstruit(sMessage, <§$000d§>, sParamètre4)
      		sMessage += RC +RC + <§$000e§>
      		
      	autres cas
      		// Le cas n'est pas géré
      		sMessage = ""
      FIN
      
      
      // mise à jour du libellé
      LIB_DESCRIPTION = sMessage
     type : 458752
   -
     name : SauveParamètres
     procedure_id : 1486460470018161688
     type_code : 14
     code : |1-
      // Syntaxe :
      //SauveParamètres ()
      //
      // Cette fonction permet de sauvegarder les paramètres actuels en fonction du type de sauvegarde défini
      
      Procédure SauveParamètres()
      
      sCheminSauvegarde est une chaine // Chemin de la sauvegarde
      nTypeSauvegarde est un entier // Type de la sauvegarde
      
      
      // Initialisation du mode de sauvegarde
      selon SEL_TYPE_SAVE
      	
      	cas 1 // Base de registre		
      		nTypeSauvegarde = paramRegistre   
      	    sCheminSauvegarde = CS_REGISTRE
      	    
      	cas 2 // Fichier XML	
      		nTypeSauvegarde = paramxml	
      		sCheminSauvegarde = CS_FICHIER_XML
      		
      	cas 3 // Fichier INI	
      		nTypeSauvegarde = Paramini
      		sCheminSauvegarde = CS_FICHIER_INI
      	
      	autres cas 
      		// Non géré
      		retour
      FIN
      
      
      // Sauvegarde automatique ou manuelle
      selon SEL_GESTION_SAVE
      	
      	cas 1 // Automatique
      		sCheminSauvegarde = ""
      	
      	cas 2 // Manuelle
      		// On ne change pas
      	
      FIN
      
      // Sauvegarde des paramètres 
      pGlobales.gnTypeSauvegarde = nTypeSauvegarde
      pGlobales.gsCheminSauvegarde = sCheminSauvegarde
      
      ferme()
     type : 458752
